{
  "name": "modal-rt",
  "version": "0.17.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "private": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "watch": "tsdx --watch",
    "build": "rm -rf dist && tsdx build --format cjs,esm",
    "lint": "tsdx lint --fix",
    "setup": "pnpm i && cd site && pnpm i && cd .. && pnpm run link",
    "link": "pnpm link ./site/node_modules/react && pnpm link ./site/node_modules/react-dom",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Abdul Kader <abdulkadernow@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iabdulkader/modal-rt.git"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "5.5 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "5 KB"
    },
    {
      "path": "headless/index.js",
      "limit": "2 KB"
    },
    {
      "path": "headless/index.mjs",
      "limit": "2 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.2",
    "@types/react": "^18.0.26",
    "goober-autoprefixer": "^1.2.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {}
}
