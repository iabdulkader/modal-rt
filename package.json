{
  "name": "modal-rt",
  "version": "1.1.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "headless",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "watch": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "size": "size-limit"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Abdul Kader <abdulkadernow@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iabdulkader/modal-rt.git"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "5.5 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "5 KB"
    },
    {
      "path": "headless/index.js",
      "limit": "2 KB"
    },
    {
      "path": "headless/index.mjs",
      "limit": "2 KB"
    }
  ],
  "devDependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "goober": "^2.1.11"
  }
}
