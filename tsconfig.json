{
	"include": ["src", "types", "test"],
	"compilerOptions": {
	  "module": "esnext",
	  "lib": ["dom", "esnext"],
	  "declaration": true,
	  "sourceMap": true,
	  "rootDir": "./src",
	  "strict": true,
	  "noImplicitReturns": true,
	  "noFallthroughCasesInSwitch": true,
	  "moduleResolution": "node",
	  "jsx": "react",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "noEmit": true,
	}
  }